<Window x:Class="GraduateWork.Client.UI.TableWindows.StudentDisciplinesWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		mc:Ignorable="d"
		Title="StudentDisciplinesWindow" Height="300" Width="300">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="*" />
			<RowDefinition Height="Auto" />
		</Grid.RowDefinitions>

		<DataGrid Grid.Row="0" Name="DataGridDisciplines" AutoGenerateColumns="False" />
		<Button Grid.Row="1" Content="Ок" Width="100" Height="30" Margin="5,10,5,5"
				Click="ButtonOk_OnClick" HorizontalAlignment="Right" />
	</Grid>
</Window>